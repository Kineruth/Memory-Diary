<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.memoryDiary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
	    android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
       <activity android:name="com.memoryDiary.Activity.Start.welcomeActivity">
           <intent-filter>
               <category android:name="android.intent.category.LAUNCHER" />
           </intent-filter>
       </activity>
       <activity android:name="com.memoryDiary.Activity.Start.LoginActivity"></activity>
       <activity android:name="com.memoryDiary.Activity.Start.RegistrationActivity"></activity>
       <activity android:name="com.memoryDiary.Activity.Start.ResetPasswordActivity"></activity>
    </application>

</manifest>
